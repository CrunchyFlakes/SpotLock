<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="300.0" maxWidth="400.0" minHeight="300.0" minWidth="400.0" prefHeight="300.0" prefWidth="400.0" style="-fx-background-color: #FFF; -fx-border-color: #FFF;" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.LockController">
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="92.0" layoutY="14.0" prefHeight="37.0" prefWidth="216.0" style="-fx-background-color: linear-gradient(#8BC34A 0%, #33691E 100%); -fx-border-color: #FFFFFF;" text="Lock Spotify Volume" textAlignment="CENTER">
            <font>
                <Font name="Open Sans Bold" size="20.0" />
            </font>
        </Label>
        <Button fx:id="lockButton" layoutX="122.0" layoutY="113.0" mnemonicParsing="false" onAction="#lockButtonHandler" prefHeight="75.0" prefWidth="157.0" text="Lock" />
        <MenuButton fx:id="menuButton" layoutX="14.0" layoutY="261.0" mnemonicParsing="false" text="loading">
            <items>
                <MenuItem fx:id="logoutItem" mnemonicParsing="false" onAction="#logoutItemHandler" text="Logout" />
            </items>
        </MenuButton>
    </children>
</Pane>
